{
  "name": "koishi-plugin-translator-deepl",
  "version": "1.0.0",
  "description": "Use Deepl translation in Koishi",
  "keywords": [
    "chatbot",
    "deepl",
    "koishi",
    "plugin",
    "translation"
  ],
  "homepage": "https://github.com/DreamOfIce/koishi-plugin-translator-deepl#readme",
  "bugs": {
    "url": "https://github.com/DreamOfIce/koishi-plugin-translator-deepl/issues"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/DreamOfIce/koishi-plugin-translator-deepl.git"
  },
  "license": "MIT",
  "author": {
    "name": "冰雪殇璃陌梦",
    "email": "admin@dreamofice.cn",
    "url": "https://www.dreamofice.cn"
  },
  "main": "./lib/index.js",
  "module": "./lib/index.mjs",
  "types": "./lib/index.d.ts",
  "files": [
    "lib",
    "dist"
  ],
  "scripts": {
    "build": "tsup",
    "postinstall": "husky install",
    "lint": "yarn lint:eslint; yarn lint:prettier",
    "lint:eslint": "eslint --ext .ts --fix ./src",
    "lint:prettier": "prettier --write .",
    "prepack": "pinst --disable",
    "postpack": "pinst --enable",
    "release": "dotenv release-it",
    "test": "echo \"Error: no test specified\" && exit 1"
  },
  "resolutions": {
    "ts-node": "npm:@dreamofice/ts-node@^10.9.2"
  },
  "dependencies": {
    "@koishijs/translator": "^1.1.0",
    "fasttext.wasm": "^1.0.1"
  },
  "devDependencies": {
    "@commitlint/cli": "^17.6.6",
    "@commitlint/config-conventional": "^17.6.6",
    "@koishijs/eslint-config": "^1.0.3",
    "@release-it/conventional-changelog": "^6.0.0",
    "@tsconfig/esm": "^1.0.4",
    "@tsconfig/strictest": "^2.0.1",
    "@typescript-eslint/eslint-plugin": "^5.61.0",
    "@typescript-eslint/parser": "^5.61.0",
    "dotenv-cli": "^7.2.1",
    "eslint": "^8.46.0",
    "eslint-config-prettier": "^8.10.0",
    "eslint-plugin-import": "^2.28.0",
    "husky": "^8.0.3",
    "koishi": "^4.14.0",
    "nano-staged": "^0.8.0",
    "pinst": "^3.0.0",
    "prettier": "^3.0.1",
    "prettier-plugin-packagejson": "^2.4.5",
    "release-it": "^16.1.3",
    "tsup": "^7.2.0",
    "typescript": "^5.1.6"
  },
  "peerDependencies": {
    "koishi": "^4.13.6"
  },
  "koishi": {
    "browser": true,
    "description": {
      "en": "Deepl translation",
      "zh": "使用 Deepl 翻译"
    },
    "service": {
      "implements": [
        "translator"
      ]
    }
  }
}
